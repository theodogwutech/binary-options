{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "ignoreCommand": "bash -c 'exit 1'",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
